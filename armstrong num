#include<stdio.h>
int main()
{
	int num,orginalnum,rem,result=0;
	printf("enter a three digit number");
	scanf("%d",&num);
	orginalnum=num;
	while(orginalnum!=0){
		rem=orginalnum%10;
		result=rem*rem*rem;
		orginalnum/=10;
		
	}
	if(result==num)
	printf("%d is an armstrong number",num);
	else
	printf("%d is  an armstrong number",num);
	return 0;
}
